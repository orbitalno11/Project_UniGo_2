<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".Home">

    <!-- TODO: Update blank fragment layout -->

    <ImageButton
        android:id="@+id/map_btn"
        android:layout_marginTop="250dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/map_btn"
        android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/dept_btn"
        android:layout_below="@id/map_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/dept_btn"
        android:background="@android:color/transparent"/>

    <ImageButton
        android:id="@+id/ebus"
        android:layout_below="@id/dept_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/ebus_btn"
        android:background="@android:color/transparent" />

    <ImageButton
        android:id="@+id/sevice"
        android:layout_below="@id/ebus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/service_btn"
        android:background="@android:color/transparent"/>

    <ImageButton
        android:id="@+id/newacis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="46dp"
        android:background="@android:color/transparent"
        android:src="@drawable/btn_newacis" />

    <ImageButton
        android:id="@+id/leb2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="45dp"
        android:background="@android:color/transparent"
        android:src="@drawable/btn_leb2" />


</RelativeLayout>